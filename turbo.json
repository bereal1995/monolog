{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "blog#build": {
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_FATHOM_ID", "NEXT_PUBLIC_POSTHOG_ID", "TWITTER_ACCESS_TOKEN"],
      "outputs": [".next/**"]
    },
    "hh-example#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
