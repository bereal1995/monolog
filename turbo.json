{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "blog#build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_NOTION_API_KEY",
        "NEXT_PUBLIC_NOTION_PAGE_DATABASE_ID",
        "NEXT_PUBLIC_NOTION_POSTS_DATABASE_ID",
        "NEXT_PUBLIC_NOTION_ABOUT_DATABASE_ID",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY"
      ],
      "outputs": [".next/**"]
    },
    "jojo#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
